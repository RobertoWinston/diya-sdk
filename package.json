{
  "name": "maya-client",
  "version": "0.0.1",
  "description": "client side code for maya",
  "main": "src/maya-client.js",
  "scripts": {
    "build-debug": "browserify src/maya-client.js -d --s maya > build/maya-client.js",
    "build-min": "browserify src/maya-client.js --s maya | uglifyjs -c > build/maya-client.min.js",
    "build": "npm run build-debug && npm run build-min",
    "watch": "watchify src/maya-client.js -d --s maya -o build/maya-client.js -v"
  },
  "author": "Sylvain Mah√© <sylv.mahe@gmail.com> (https://github.com/nilpotence)",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "browserify": "^5.12.0",
    "uglify-js": "^2.4.15",
    "watchify": "^1.0.5"
  }
}
