{
  "name": "diya-sdk",
  "version": "0.5.0",
  "description": "SDK for accessing Diya One services",
  "main": "src/diya-sdk.js",
  "scripts": {
    "build-debug": "browserify src/diya-sdk.js -d --s d1 > dist/diya-sdk.js",
    "build-min": "browserify src/diya-sdk.js --s d1 | uglifyjs -c > dist/diya-sdk.min.js",
    "build": "npm run build-debug && npm run build-min",
    "watch": "watchify src/diya-sdk.js -d --s d1 -o dist/diya-sdk.js -v"
  },
  "author": "Sylvain Mah√© <sylv.mahe@gmail.com> (https://github.com/nilpotence)",
  "license": "MIT",
  "dependencies": {
    "c3": "^0.4.9",
    "q": "^1.4.1"
  },
  "devDependencies": {
    "browserify": "^5.12.0",
    "uglify-js": "^2.4.15",
    "watchify": "^1.0.5"
  }
}
